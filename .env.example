APP_LOG_FORMAT="%(asctime)s\t%(levelname)s\t[%(name)s]\t%(message)s"

APP_DOCS_URL="/api/docs"
APP_REDOC_URL="/api/redoc"
APP_OPENAPI_URL="/api/openapi.json"
